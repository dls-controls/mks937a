# Pressure
#
#% archiver 0.1 Monitor
#
record(ai, "$(dom)-VA-GAUGE-$(id):PLOG") {
  field(DESC, "log Pressure")
  field(DTYP, "Hy8401ip")
  field(INP, "#C$(c) S$(s) @")
  field(EGU, "log")
  field(LINR, "LINEAR")
