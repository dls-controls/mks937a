  field(EGUF, "4.72")
  field(EGUL, "-28.48")
  field(HOPR, "3")
  field(LOPR, "-11")
  field(HIGH, "-7")
  field(HSV, "MINOR")
  field(ADEL, "0.025")
}

#
#% controldesk Pressure readback
#% archiver 0.1 Monitor
#
record(calc, "$(dom)-VA-GAUGE-$(id):P") {
  field(DESC, "Pressure")
  field(EGU, "mbar")
  field(PREC, "11")
  field(CALC, "10^A")
  field(INPA, "$(dom)-VA-GAUGE-$(id):PLOG PP MS")
  field(SCAN, ".1 second")
  field(HOPR, "1000")
  field(LOPR, "0")
}

record(calcout, "$(dom)-VA-GAUGE-$(id):ADEL") {
  field(DESC, "deadband")
  field(CALC, "A/B")
  field(OUT, "$(dom)-VA-GAUGE-$(id):P.ADEL")
  field(INPA, "$(dom)-VA-GAUGE-$(id):P")
  field(INPB, "20")
  field(SCAN, "10 second")
}

